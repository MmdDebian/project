// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model User {
  id String @id @default(uuid())
  avatar String?
  first_name String?
  last_name String?
  email String? @unique
  password String? 
  phoneNumber String @unique
  bio String? 

}